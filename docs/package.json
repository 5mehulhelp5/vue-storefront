{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "MIT",
  "scripts": {
    "dev": "vuepress dev",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 vuepress build",
    "core-cache-ref": "cd ../packages/core/cache && api-extractor run --local",
    "core-core-ref": "cd ../packages/core/core && api-extractor run --local",
    "core-middleware-ref": "cd ../packages/core/middleware && api-extractor run --local",
    "core-ref-md": "api-documenter markdown --i core/api-reference --o core/api-reference",
    "build:core": "yarn core-cache-ref && yarn core-core-ref && yarn core-middleware-ref && yarn core-ref-md",
    "ct-api-ref": "cd ../packages/commercetools/api-client && api-extractor run --local",
    "ct-comp-ref": "cd ../packages/commercetools/composables && api-extractor run --local",
    "ct-ref-md": "api-documenter markdown --i commercetools/api-reference --o commercetools/api-reference",
    "build:ct": "yarn ct-api-ref && yarn ct-comp-ref && yarn ct-ref-md"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.7",
    "@microsoft/api-extractor": "^7.18.3",
    "@types/node": "^16.11.6",
    "@vuepress/bundler-vite": "^2.0.0-beta.27",
    "@vuepress/plugin-git": "^2.0.0-beta.27",
    "@vuepress/plugin-medium-zoom": "^2.0.0-beta.27",
    "@vuepress/plugin-register-components": "^2.0.0-beta.27",
    "@vuepress/plugin-search": "^2.0.0-beta.27",
    "handlebars": "^4.7.7",
    "node-html-to-image": "^3.2.0",
    "vuepress": "^2.0.0-beta.27"
  },
  "dependencies": {
    "vue-multiselect": "^3.0.0-alpha.2"
  }
}
