{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "MIT",
  "scripts": {
    "dev": "vuepress dev",
    "build": "pnpm build:core && NODE_OPTIONS=--max_old_space_size=8192 vuepress build",
    "ref-cache": "cd ../cache && api-extractor run --local",
    "ref-core": "cd ../core && api-extractor run --local",
    "ref-middleware": "cd ../middleware && api-extractor run --local",
    "ref-nuxt-module": "cd ../nuxt-module && api-extractor run --local",
    "ref-md": "api-documenter markdown --i reference/api --o reference/api",
    "build:core": "pnpm ref-cache && pnpm ref-core && pnpm ref-middleware && pnpm ref-nuxt-module && pnpm ref-md",
    "build:changelog": "cd scripts && node changelog --in ../changelog --out ../reference/changelog.md"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.17.26",
    "@vuepress/plugin-active-header-links": "^1.8.2",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-medium-zoom": "^1.8.2",
    "@vuepress/plugin-search": "^1.8.2",
    "handlebars": "^4.7.7",
    "markdown-it-video": "^0.6.3",
    "node-html-to-image": "^3.2.0",
    "vuepress": "^1.9.7"
  },
  "dependencies": {
    "vue-multiselect": "^2.1.6"
  }
}
