{
  "name": "@vue-storefront/cli",
  "version": "2.4.1",
  "bin": {
    "init": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "cli": "./bin/run",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "init",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "license": "MIT",
  "repository": "vuestorefront/vue-storefront",
  "homepage": "https://github.com/vuestorefront/vue-storefront",
  "bugs": "https://github.com/vuestorefront/vue-storefront/issues",
  "dependencies": {
    "axios": "^0.23.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-update": "^1.5.0",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@vue-storefront/theme-utilities": "^0.1.3",
    "chokidar": "^3.3.1",
    "consola": "^2.10.1",
    "execa": "^5.0.0",
    "inquirer": "^7.3.3",
    "isomorphic-git": "^1.9.2",
    "rimraf": "^3.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "types": "lib/index.d.ts"
}
