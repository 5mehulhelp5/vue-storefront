{
  "name": "@vue-storefront/cli",
  "version": "2.4.1",
  "bin": {
    "init": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "cli": "./bin/run",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "init",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "license": "MIT",
  "repository": "vuestorefront/vue-storefront",
  "homepage": "https://github.com/vuestorefront/vue-storefront",
  "bugs": "https://github.com/vuestorefront/vue-storefront/issues",
  "dependencies": {
    "axios": "^0.23.0",
    "@oclif/command": "^1.8.0",
    "cli-ux": "^5.6.3",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/plugin-update": "^1.5.0",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@vue-storefront/theme-utilities": "^0.1.6",
    "chokidar": "^3.5.2",
    "consola": "^2.15.3",
    "chalk": "^4.1.2",
    "execa": "^5.1.1",
    "inquirer": "^8.2.0",
    "os-locale": "^5.0.0",
    "isomorphic-git": "^1.10.1",
    "rimraf": "^3.0.2",
    "tildify": "^2.0.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/rimraf": "^3.0.2",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/node": "^14.17.27",
    "globby": "^10",
    "ts-node": "^8.4.1",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "types": "lib/index.d.ts"
}
