{
  "name": "@vue-storefront/cli",
  "version": "0.0.0",
  "description": "Vue Storefront's CLI.",
  "bin": "./bin/run",
  "homepage": "https://github.com/vuestorefront/vue-storefront",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "vuestorefront/vue-storefront",
  "files": [
    "/bin",
    "/dist",
    "/locales",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.3.2",
    "@oclif/plugin-help": "^5.1.11",
    "@oclif/plugin-plugins": "^2.1.0",
    "@vue-storefront/theme-utilities": "^0.1.7",
    "execa": "^5.1.1",
    "i18next": "^21.6.11",
    "i18next-fs-backend": "^1.1.4",
    "inquirer": "^8.2.0",
    "isomorphic-git": "^1.11.2",
    "os-locale": "^5.0.0",
    "reasonable-filename": "^1.2.0"
  },
  "devDependencies": {
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^16.11.7",
    "eslint": "^7.32.0",
    "globby": "^11.1.0",
    "oclif": "^2.4.3",
    "shx": "^0.3.4",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "oclif": {
    "bin": "@vue-storefront/cli",
    "dirname": "@vue-storefront/cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "commands": "./dist/commands",
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && shx rm tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest --rootDir .",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/vuestorefront/vue-storefront/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
