{
  "name": "@vue-storefront/core",
  "version": "2.5.4",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./lib/index.mjs",
  "exports": {
    ".": "./lib/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "build": "yarn build:package && yarn build:types",
    "build:package": "node ../../../scripts/build",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "node ../../../scripts/build --watch",
    "test": "vitest run",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "axios": "0.21.1",
    "express": "^4.17.1",
    "is-https": "^3.0.2",
    "lodash-es": "^4.17.15",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.7",
    "@types/express": "^4.11.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2"
  },
  "peerDependencies": {
    "@nuxtjs/composition-api": "^0.29.3"
  },
  "files": [
    "lib/**/*",
    "server/**/*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
