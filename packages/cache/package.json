{
  "name": "@vue-storefront/cache",
  "version": "2.5.5",
  "license": "MIT",
  "sideEffects": false,
  "main": "./lib/index.mjs",
  "exports": {
    ".": "./lib/index.mjs",
    "./nuxt": "./nuxt/index.js",
    "./defaultHandler": "./defaultHandler/index.js",
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "build": "yarn build:clear && yarn build:package && yarn build:types",
    "build:clear": "rimraf lib",
    "build:package": "node ../../scripts/build --file=src/index.ts",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "yarn build:package --watch",
    "test": "jest",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "@vue-storefront/core": "~2.5.5"
  },
  "peerDependencies": {
    "@nuxtjs/composition-api": "^0.29.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*",
    "defaultHandler/**/*",
    "nuxt/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuestorefront/vue-storefront.git"
  },
  "bugs": {
    "url": "https://github.com/vuestorefront/vue-storefront/issues"
  },
  "homepage": "https://github.com/vuestorefront/vue-storefront#readme"
}
